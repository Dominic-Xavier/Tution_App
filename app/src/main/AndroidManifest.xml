<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tutionapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/pngtree_school_education_logo_png_image_4136147"
        android:label="@string/app_name"
        android:roundIcon="@drawable/pngtree_school_education_logo_png_image_4136147"
        android:supportsRtl="true"
        android:theme="@style/Theme.TutionApp">
        <activity android:name="com.register_Login.Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.register_Login.Registration_Parents"/>
        <activity android:name="com.register_Login.Student_Registration"/>
        <activity android:name="com.register_Login.Teacher_Registration"/>
        <activity android:name="com.register_Login.WhoAreYou"/>
        <activity android:name="com.tutionapp.TutionActivity"/>
        <activity android:name="com.register_Login.Student"/>
        <activity android:name="com.owner.AllStudentsList"/>

        <activity android:name="com.register_Login.OTPValidation"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustResize"/>

    </application>

</manifest>