<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tutionapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TutionApp">
        <activity android:name="com.register_Login.Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.register_Login.Registration_Parents"/>
        <activity android:name="com.register_Login.Student_Registration"/>
        <activity android:name="com.register_Login.Teacher_Registration"/>
        <activity android:name="com.register_Login.WhoAreYou"/>
        <activity android:name="com.register_Login.Registeration"/>
    </application>

</manifest>